{

    "manifest_version": 2,
    "name": "Motiv8 Tracker",
    "version": "1.0",
  
    "description": "A simple time tracker extension",
    "homepage_url": "https://github.com/DuceTony/GP_01",
  
    "browser_action": {
      "default_title": "Motiv8 Tracker",
      "default_popup": "popup/popup.html"
    },
  
    "background": {
      "scripts": ["background.js"]
    },
  
    "permissions": [
      "alarms"
    ]
  
  }
 